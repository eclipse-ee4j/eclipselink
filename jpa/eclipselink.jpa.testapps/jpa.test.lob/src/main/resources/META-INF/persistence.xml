<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence persistence_1_0.xsd" version="1.0">
    <persistence-unit name="default" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
<!--        <class>org.eclipse.persistence.testing.models.jpa.lob.Image</class>-->
<!--        <class>org.eclipse.persistence.testing.models.jpa.lob.ImageSimulator</class>-->
<!--        <class>org.eclipse.persistence.testing.models.jpa.lob.SerializableNonEntity</class>-->
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="eclipselink.ddl-generation" value="drop-and-create-tables"/>
        </properties>
    </persistence-unit>
</persistence>
