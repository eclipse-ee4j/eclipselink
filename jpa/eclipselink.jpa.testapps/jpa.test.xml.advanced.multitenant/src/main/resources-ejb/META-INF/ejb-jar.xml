<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
          https://jakarta.ee/xml/ns/jakartaee/ejb-jar_4_0.xsd"
         version="4.0">
    <enterprise-beans>
        <session>
            <ejb-name>GenericTestRunner</ejb-name>
            <persistence-context-ref>
                <persistence-context-ref-name>persistence/extended-multi-tenant-shared-emf/entity-manager</persistence-context-ref-name>
                <persistence-unit-name>extended-multi-tenant-shared-emf</persistence-unit-name>
            </persistence-context-ref>
            <persistence-context-ref>
                <persistence-context-ref-name>persistence/extended-multi-tenant-123/entity-manager</persistence-context-ref-name>
                <persistence-unit-name>extended-multi-tenant-123</persistence-unit-name>
            </persistence-context-ref>
            <persistence-context-ref>
                <persistence-context-ref-name>persistence/extended-multi-tenant-table-per-tenant/entity-manager</persistence-context-ref-name>
                <persistence-unit-name>extended-multi-tenant-table-per-tenant</persistence-unit-name>
            </persistence-context-ref>
            <persistence-context-ref>
                <persistence-context-ref-name>persistence/extended-multi-tenant-table-per-tenant-C/entity-manager</persistence-context-ref-name>
                <persistence-unit-name>extended-multi-tenant-table-per-tenant-C</persistence-unit-name>
            </persistence-context-ref>

            <persistence-unit-ref>
                <persistence-unit-ref-name>persistence/extended-multi-tenant-shared-emf/factory</persistence-unit-ref-name>
                <persistence-unit-name>extended-multi-tenant-shared-emf</persistence-unit-name>
            </persistence-unit-ref>
            <persistence-unit-ref>
                <persistence-unit-ref-name>persistence/extended-multi-tenant-123/factory</persistence-unit-ref-name>
                <persistence-unit-name>extended-multi-tenant-123</persistence-unit-name>
            </persistence-unit-ref>
            <persistence-unit-ref>
                <persistence-unit-ref-name>persistence/extended-multi-tenant-table-per-tenant/factory</persistence-unit-ref-name>
                <persistence-unit-name>extended-multi-tenant-table-per-tenant</persistence-unit-name>
            </persistence-unit-ref>
            <persistence-unit-ref>
                <persistence-unit-ref-name>persistence/extended-multi-tenant-table-per-tenant-C/factory</persistence-unit-ref-name>
                <persistence-unit-name>extended-multi-tenant-table-per-tenant-C</persistence-unit-name>
            </persistence-unit-ref>

        </session>

    </enterprise-beans>
</ejb-jar>