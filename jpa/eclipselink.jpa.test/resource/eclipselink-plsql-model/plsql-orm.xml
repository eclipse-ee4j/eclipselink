                <column name="ADDRESS_ID"/>
            </basic>
            <basic name="number">
                <column name="STREET_NUM"/>
            </basic>
        </attributes>
    </embeddable>
    <embeddable class="Phone" access="FIELD">
        <struct name="PLSQL_P_PLSQL_PHONE_REC">
                <field>AREA_CODE</field>
                <field>P_NUM</field>
        </struct>
        <attributes>
            <basic name="areaCode">
                <column name="AREA_CODE"/>
            </basic>
            <basic name="number">
                <column name="P_NUM"/>
            </basic>
        </attributes>
    </embeddable>
</entity-mappings>
