<datasources>
    <local-tx-datasource>
        <jndi-name>EclipseLinkDS</jndi-name>
        <connection-url>@url@</connection-url>
        <driver-class>@driverclass@</driver-class>
        <user-name>@username@</user-name>
        <password>@password@</password>
        <metadata>
            <type-mapping>MS SQLSERVER2000</type-mapping>
        </metadata>
    </local-tx-datasource>
</datasources>
