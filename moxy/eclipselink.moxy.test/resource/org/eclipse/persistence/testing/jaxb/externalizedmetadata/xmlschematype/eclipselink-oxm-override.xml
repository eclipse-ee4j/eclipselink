<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm">
    <xml-schema-type name="time" type="java.util.GregorianCalendar" />
    <java-types>
        <java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.xmlschematype.Employee">
            <xml-root-element name="employee" />
            <java-attributes>
                <xml-attribute java-attribute="id" xml-id="true">
                    <xml-schema-type name="string" />
                </xml-attribute>
                <xml-element java-attribute="hireDate">
