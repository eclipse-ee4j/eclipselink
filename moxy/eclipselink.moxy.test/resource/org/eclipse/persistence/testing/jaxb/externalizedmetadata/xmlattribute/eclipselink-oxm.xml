<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm">
    <java-types>
        <java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.xmlattribute.Employee">
            <java-attributes>
                <xml-attribute java-attribute="firstName" name="fname" type="java.lang.String" />
                <xml-attribute java-attribute="lastName" name="lname" />
                <xml-attribute java-attribute="id" required="true" />
                <xml-attribute java-attribute="things" type="java.lang.String" container-type="java.util.LinkedList" />
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>
