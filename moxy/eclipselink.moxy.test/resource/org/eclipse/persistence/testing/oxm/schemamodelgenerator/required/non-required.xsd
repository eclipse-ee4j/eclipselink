<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:element name="required-test-object">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="direct" type="xsd:string" minOccurs="0"/>
            <xsd:element name="directCollection" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="compositeObject" minOccurs="0">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="value" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="compositeCollection" minOccurs="0" maxOccurs="unbounded">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="value" type="xsd:string" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
         </xsd:sequence>
         <xsd:attribute name="directAttribute" type="xsd:string"/>
      </xsd:complexType>
   </xsd:element>
</xsd:schema>
