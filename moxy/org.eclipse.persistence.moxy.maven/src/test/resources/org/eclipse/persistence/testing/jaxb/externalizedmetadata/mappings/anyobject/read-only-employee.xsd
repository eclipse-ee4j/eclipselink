<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:complexType name="employee">
      <xsd:sequence>
         <xsd:element name="read-only" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="stuff" minOccurs="0">
                     <xsd:complexType mixed="true">
                        <xsd:sequence>
                           <xsd:any processContents="skip" namespace="##other" />
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="employee" type="employee"/>
</xsd:schema>
