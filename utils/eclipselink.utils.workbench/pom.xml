                <groupId>org.eclipse.persistence</groupId>
                <artifactId>workbench.uitools</artifactId>
                <version>2.7.3-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>org.eclipse.persistence</groupId>
                <artifactId>workbench.framework</artifactId>
                <version>2.7.3-SNAPSHOT</version>
            </dependency>
