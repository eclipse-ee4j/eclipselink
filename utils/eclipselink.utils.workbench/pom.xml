        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
    </properties>
    <modules>
        <module>config</module>
        <module>iconlib</module>
        <module>utility</module>
        <module>uitools</module>
