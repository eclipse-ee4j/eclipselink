<sessions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://xsd/eclipselink_sessions_1.0.xsd" version="0">
    <session xsi:type="database-session">
        <name>NullOptionsSession</name>
        <logging xsi:type="eclipselink-log">
            <log-level>fine</log-level>
            <file-name>logging-options-null.log</file-name>
            <logging-options>
            </logging-options>
        </logging>
        <primary-project xsi:type="class">org.eclipse.persistence.testing.models.employee.relational.EmployeeProject</primary-project>
        <additional-project xsi:type="xml">org/eclipse/persistence/testing/models/insurance/insurance.xml</additional-project>
        <additional-project xsi:type="xml">testDeployment.xml</additional-project>
        <login xsi:type="database-login">
            <user-name>@dbUser@</user-name>
            <password>@dbPassword@</password>
            <driver-class>@driverClass@</driver-class>
            <connection-url>@dbURL@</connection-url>
        </login>
    </session>
    <session xsi:type="database-session">
        <name>DefaultSession</name>
        <logging xsi:type="eclipselink-log">
            <log-level>fine</log-level>
            <file-name>logging-options-default.log</file-name>
            <logging-options>
                <log-exception-stacktrace/>
                <print-thread/>
                <print-session/>
                <print-connection/>
                <print-date/>
            </logging-options>
        </logging>
        <primary-project xsi:type="class">org.eclipse.persistence.testing.models.employee.relational.EmployeeProject</primary-project>
        <additional-project xsi:type="xml">org/eclipse/persistence/testing/models/insurance/insurance.xml</additional-project>
        <additional-project xsi:type="xml">testDeployment.xml</additional-project>
        <login xsi:type="database-login">
            <user-name>@dbUser@</user-name>
            <password>@dbPassword@</password>
            <driver-class>@driverClass@</driver-class>
            <connection-url>@dbURL@</connection-url>
        </login>
    </session>
    <session xsi:type="database-session">
        <name>TrueSession</name>
        <logging xsi:type="eclipselink-log">
            <log-level>fine</log-level>
            <file-name>logging-options-true.log</file-name>
            <logging-options>
                <log-exception-stacktrace>true</log-exception-stacktrace>
                <print-thread>true</print-thread>
                <print-session>true</print-session>
                <print-connection>true</print-connection>
                <print-date>true</print-date>
            </logging-options>
        </logging>
        <primary-project xsi:type="class">org.eclipse.persistence.testing.models.employee.relational.EmployeeProject</primary-project>
        <additional-project xsi:type="xml">org/eclipse/persistence/testing/models/insurance/insurance.xml</additional-project>
        <additional-project xsi:type="xml">testDeployment.xml</additional-project>
        <login xsi:type="database-login">
            <user-name>@dbUser@</user-name>
            <password>@dbPassword@</password>
            <driver-class>@driverClass@</driver-class>
            <connection-url>@dbURL@</connection-url>
        </login>
    </session>
    <session xsi:type="database-session">
        <name>FalseSession</name>
        <logging xsi:type="eclipselink-log">
            <log-level>fine</log-level>
            <file-name>logging-options-false.log</file-name>
            <logging-options>
                <log-exception-stacktrace>false</log-exception-stacktrace>
                <print-thread>false</print-thread>
                <print-session>false</print-session>
                <print-connection>false</print-connection>
                <print-date>false</print-date>
            </logging-options>
        </logging>
        <primary-project xsi:type="class">org.eclipse.persistence.testing.models.employee.relational.EmployeeProject</primary-project>
        <additional-project xsi:type="xml">org/eclipse/persistence/testing/models/insurance/insurance.xml</additional-project>
        <additional-project xsi:type="xml">testDeployment.xml</additional-project>
        <login xsi:type="database-login">
            <user-name>@dbUser@</user-name>
            <password>@dbPassword@</password>
            <driver-class>@driverClass@</driver-class>
            <connection-url>@dbURL@</connection-url>
        </login>
    </session>
</sessions>
